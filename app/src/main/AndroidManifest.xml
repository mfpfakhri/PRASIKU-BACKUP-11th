<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="clouwiko.dev.prasiku">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL" />

    <uses-feature android:name="android.hardware.camera2" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.activity.SignInActivity"
            android:parentActivityName=".activity.activity.LandingActivity" />
        <activity
            android:name=".activity.activity.SignUpActivity"
            android:parentActivityName=".activity.activity.LandingActivity" />
        <activity
            android:name=".activity.activity.ResetPasswordActivity"
            android:parentActivityName=".activity.activity.LandingActivity" />
        <activity
            android:name=".activity.activity.MainMenuActivity"
            android:clearTaskOnLaunch="true" />
        <activity android:name=".activity.activity.ChangePasswordActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity android:name=".activity.activity.EditUserProfileActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".activity.activity.AddProvinceActivity" />
        <activity android:name=".activity.activity.AddCityActivity" />
        <activity
            android:name=".activity.activity.UserHomeActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.AppReceivedActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.AppAcceptedActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.TutorialActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.AboutUsActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.UploadCatDataActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.FindCatForAdoptActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.UserAdoptionListActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity android:name=".activity.activity.CatProfileApplicantAdoptedActivity" />
        <activity android:name=".activity.activity.CatProfileApplicantAvailableActivity" />
        <activity android:name=".activity.activity.CatProfileOwnerAdoptedActivity" />
        <activity android:name=".activity.activity.CatProfileOwnerAvailableActivity" />
        <activity android:name=".activity.activity.ApplicantAdoptionFormActivity" />
        <activity android:name=".activity.activity.EditCatDataAdoptedActivity" />
        <activity android:name=".activity.activity.EditCatDataAvailableActivity" />
        <activity
            android:name=".activity.activity.AppRejectedActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity
            android:name=".activity.activity.UserApplicationListActivity"
            android:parentActivityName=".activity.activity.MainMenuActivity" />
        <activity android:name=".activity.activity.UserApplicationListReviewActivity" />
        <activity android:name=".activity.activity.AppReceivedReviewActivity" />
        <activity android:name=".activity.activity.AppAcceptedReviewActivity" />
        <activity android:name=".activity.activity.AppRejectedReviewActivity" />
        <activity android:name=".activity.activity.VerificationActivity"></activity>
    </application>

</manifest>